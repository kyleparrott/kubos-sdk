{
    "install_requires": [
        "yotta",
        "docker-py==1.9.0",
        "dockerpty",
        "psutil",
        "setuptools",
        "requests==2.10.0",
        "pager",
        "appdirs"
    ],
    "classifiers": [
        "Programming Language :: Python :: 2.7"
    ],
    "entry_points": {
        "console_scripts": [
            "kubos=kubos:main"
        ]
    },
    "test_suite": "kubos.test",
    "package_dir": {"kubos": "kubos"},
    "package_data": {
        "kubos": ["*.txt",
                  "*.json",
                  "flash/dfu_util/*.sh",
                  "flash/mspdebug/*.sh",
                  "flash/openocd/*",
                  "flash/openocd/interface/*",
                  "flash/openocd/target/*"]
        }
}
